{
  "format_version": "1.16.100",
  "minecraft:entity": {
    "description": {
      "identifier": "boat:huey",
      "is_summonable": true,
      "is_spawnable": false,
      "is_experimental": false,
      "scripts": {
        "animate": [
          "flying",
          "test_rider",
          "speed"
        ]
      },
      "animations": {
        "flying": "controller.animation.huey.flying",
        "test_rider": "controller.animation.huey.test_rider",
        "speed": "controller.animation.huey.speed"
      }
    },
    "component_groups": {
      "huey:fly": {
        "minecraft:movement": {
          "value": 0.6
        }
      },
      "huey:taxi": {
        "minecraft:movement": {
          "value": 0.0
        }
      }
    },
    "components": {
      "minecraft:type_family": {
        "family": [
          "boat",
          "inanimate"
        ]
      },
      "minecraft:collision_box": {
        "width": 3,
        "height": 2
      },
      "minecraft:knockback_resistance": {
        "value": 0.5,
        "max": 0.5
      },
      "minecraft:custom_hit_test": {
        "hitboxes": [
          { //front
            "width": 3.0,
            "height": 2.0,
            "pivot": [ 0.0, 1.5, 3.0 ]
          },
          { //front mid
            "width": 3.0,
            "height": 2.0,
            "pivot": [ 0.0, 1.5, 1.5 ]
          },
          { //mid mid
            "width": 3.0,
            "height": 2.0,
            "pivot": [ 0.0, 1.5, 0 ]
          },
          { //back mid
            "width": 3.0,
            "height": 2.0,
            "pivot": [ 0.0, 1.5, -1.5 ]
          }
        ]
      },
      "minecraft:break_blocks": {
        "breakable_blocks": [
          "glass"
        ]
      },
      "minecraft:rideable": {
        "seat_count": 6,
        /*"family_types": [
          "player",
          "zombie"
        ],*/
        "interact_text": "action.interact.ride.huey",
        "pull_in_entities": true,
        "seats": [
          {
            "position": [
              0.8,
              0.7,
              1.7
            ],
            "min_rider_count": 0,
            "max_rider_count": 6
          },
          {
            "position": [
              -0.8,
              0.7,
              1.7
            ],
            "min_rider_count": 2,
            "max_rider_count": 6
          },
          {
            "position": [
              1.3,
              0.5,
              0
            ],
            "min_rider_count": 3,
            "max_rider_count": 6,
            "rotate_rider_by": 90
          },
          {
            "position": [
              -1.3,
              0.5,
              0
            ],
            "min_rider_count": 4,
            "max_rider_count": 6,
            "rotate_rider_by": -90
          },
          {
            "position": [
              1.3,
              0.5,
              -1.5
            ],
            "min_rider_count": 5,
            "max_rider_count": 6,
            "rotate_rider_by": 90
          },
          {
            "position": [
              -1.3,
              0.5,
              -1.5
            ],
            "min_rider_count": 6,
            "max_rider_count": 6,
            "rotate_rider_by": -90
          }
        ]
      },
      "minecraft:physics": {},
      "minecraft:is_stackable": {},
      "minecraft:push_through": 1,
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:scale": {
        "value": 1.0
      },
      //Sets the boat speed in water
      "minecraft:underwater_movement": {
        "value": 0.0
      },
      "minecraft:water_movement": {
        "drag_factor": 0.1
      },
      "minecraft:navigation.walk": {
        "can_sink": true
      },
      //Add this component if you want your boat to be controlled with WASD
      "minecraft:input_ground_controlled": {},
      "minecraft:health": {
        "value": 40,
        "max": 40
      },
      "minecraft:damage_sensor": {
        "triggers": [
          { // Reduces Fall Damage
            "cause": "fall",
            "deals_damage": true,
            "damage_modifier": -5
          },
          { // Disables damage to the berry bushes
            "on_damage": {
              "filters": {
                "test": "is_block",
                "subject": "block",
                "value": "minecraft:sweet_berry_bush"
              }
            },
            "deals_damage": false
          },
          { // Sets bullet/arrow damage to half a heart
            "cause": "projectile",
            "deals_damage": true,
            "damage_multiplier": 0.08
          },
          { // Disables melee damage
            "cause": "attack",
            "deals_damage": false
          }
        ]
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:drying_out_timer": {
        "total_time": 2000
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": -1,
        "inhale_time": 3.75,
        "generates_bubbles": true
      },
      "minecraft:movement": {
        "value": 0.0
      },
      "minecraft:movement.basic": {},
      "minecraft:inventory": {
        "container_type": "minecart_chest",
        "inventory_size": 6
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [ "huey:taxi" ]
        }
      },
      "huey:movement": {
        "sequence": [
          {
            "remove": {
              "component_groups": [ "huey:taxi" ]
            },
            "add": {
              "component_groups": [ "huey:fly" ]
            }
          }
        ]
      },
      "huey:landed": {
        "sequence": [
          {
            "remove": {
              "component_groups": [ "huey:fly" ]
            },
            "add": {
              "component_groups": [ "huey:taxi" ]
            }
          }
        ]
      }
    }
  }
}
