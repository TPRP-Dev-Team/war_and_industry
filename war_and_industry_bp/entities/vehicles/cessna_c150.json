        ]
      },
      "animations": {
        "flying": "controller.animation.cc150.flying",
        "test_rider": "controller.animation.cc150.test_rider",
        "speed": "controller.animation.cc150.speed"
      }
    },
    "component_groups": {
      "cessna:fly": {
        "minecraft:movement": {
