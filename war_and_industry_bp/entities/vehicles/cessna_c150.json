        "width": 2.5,
        "height": 2
      },
      "minecraft:knockback_resistance": {
        "value": 0.5,
        "max": 0.5
      },
      "minecraft:break_blocks": {
        "breakable_blocks": [
          "glass"
        ]
      },
      "minecraft:rideable": {
        "seat_count": 3,
        "family_types": [
          "player"
        ],
