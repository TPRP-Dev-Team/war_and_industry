        "triggers": [
          { // Reduces Fall Damage
            "cause": "fall",
            "deals_damage": true,
            "damage_modifier": -5
          },
          { // Disables damage to the berry bushes
            "on_damage": {
              "filters": {
                "test": "is_block",
                "subject": "block",
                "value": "minecraft:sweet_berry_bush"
              }
            },
            "deals_damage": false
          },
          { // Sets bullet/arrow damage to half a heart
            "cause": "projectile",
            "deals_damage": true,
            "damage_multiplier": 0.08
          },
          { // Disables melee damage
            "cause": "attack",
            "deals_damage": false
          }
        ]
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:drying_out_timer": {
        "total_time": 2000
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": -1,
        "inhale_time": 3.75,
        "generates_bubbles": true
