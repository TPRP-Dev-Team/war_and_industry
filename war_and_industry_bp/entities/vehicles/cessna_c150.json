    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [ "cessna:taxi" ]
        }
      },
      "c150_fly": {
        "sequence": [
          {
            "remove": {
              "component_groups": [ "cessna:taxi" ]
