            "add": {
              "component_groups": [ "cessna:fly" ]
            }
          }
        ]
      },
      "c150_taxi": {
        "sequence": [
          {
            "remove": {
              "component_groups": [ "cessna:fly" ]
            },
            "add": {
              "component_groups": [ "cessna:taxi" ]
